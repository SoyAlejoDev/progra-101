{
  "name": "progra-101",
  "version": "1.0.0",
  "description": "Monorepo multipaquete para progra-101",
  "private": true,
  "type": "module",
  
  "scripts": {
    "dev": "pnpm --filter hanging dev",
    "build": "pnpm --filter hanging build",
    "preview": "pnpm --filter hanging preview",
    "lint": "pnpm --filter hanging lint",
    "deploy": "pnpm --filter hanging build && gh-pages -d apps/hanging/dist",
    "dev:ts": "ts-node app.ts",
    "dev:watch": "nodemon --watch './**/*.ts' --exec 'ts-node' app.ts"
  },
  "devDependencies": {
    "@types/node": "^24.10.9",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "gh-pages": "^6.2.0"
  },
  "packageManager": "pnpm@9.0.0"
}
